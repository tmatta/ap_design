yhat <- function(df){#
  m <- lm(df[, "y"] ~ 1 + df[, "gs"])#
  yhat <- model.matrix(m) %*% coef(m)#
  yhat <- cbind(yhat, df[, "gs"])#
  resid <- df[, "y"] - yhat#
  return(list(yhat, resid))#
}
